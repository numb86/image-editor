{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "flowtype",
    "prettier"
  ],
  "globals": {
    "document": true,
    "FileReader": false,
    "Image": false,
    "describe": false,
    "it": false
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "no-param-reassign": ["error", {"props": false}],
    "flowtype/define-flow-type": 1,
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5",
        "bracketSpacing": false
      }
    ]
  }
}